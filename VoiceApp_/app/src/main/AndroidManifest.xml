        <activity
            android:name=".TiposViolencia.IViolenciaPatrimonial"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TiposViolencia.IViolenciaPsicologica"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TiposViolencia.IViolenciaSexual"
            android:label="@string/app_name"
