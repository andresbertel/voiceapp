        <activity android:name=".RutasdeAtencion.RutaFisica" />
        <activity android:name=".RutasdeAtencion.RutaPsicologica" />
        <activity android:name=".RutasdeAtencion.RutaPatrimonial" />
        <activity android:name=".RutasdeAtencion.RutaSexual" />
        <activity android:name=".ListaContactos.ListaContactosPrincipal" />
        <activity android:name=".Apoya"
