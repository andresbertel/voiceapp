            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegistroUsuario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainPrincipal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegistroCuestionario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Cuestionario"
            android:screenOrientation="portrait" />
        <activity android:name=".Principal" />
        <activity
            android:name=".Splash"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ver_contactos" />
        <activity android:name=".ListadodeContactos" />
        <activity android:name=".TiposdeViolencia" />
        <activity android:name=".rutasdeatencion" />
        <activity
            android:name=".TiposViolencia.IViolenciaFisica"
